<p id="notice"><%= notice %></p>

<h1>Listing Current Locations</h1>

<table>
  <thead>
    <tr>
      <th>Lat</th>
      <th>Lng</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @current_locations.each do |current_location| %>
      <tr>
        <td><%= current_location.lat %></td>
        <td><%= current_location.lng %></td>
        <td><%= current_location.user_id %></td>
        <td><%= link_to 'Show', current_location %></td>
        <td><%= link_to 'Edit', edit_current_location_path(current_location) %></td>
        <td><%= link_to 'Destroy', current_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Current location', new_current_location_path %>
